{
  "eslint.enable": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "files.eol": "\n",
  "files.associations": {
    "*.css.ts": "typescriptreact"
  },
  "reactSnippets.settings.importReactOnTop": false,

  "cssvar.files": ["src/**/*.css"],
  "cssvar.extensions": ["css", "scss"],
  "cSpell.words": ["Horizen", "iconfont", "immer", "zustand"]
}
